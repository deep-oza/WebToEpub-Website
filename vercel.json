{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server.js"
    },
    {
      "src": "/src/client/(.*)",
      "dest": "/src/client/$1"
    },
    {
      "src": "/libs/(.*)",
      "dest": "/public/libs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}